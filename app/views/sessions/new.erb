<h1 class="center light-blue-text text-lighten-3">Log In</h1>
<form action = '/login' method = "POST">
 <div class="input-field">
    <label for="user">UserName: </label> 
    <input type="text" name="user[user_name]" id="user" required>
</div>
 <div class="input-field">
    <label for="pword">Password: </label> 
    <input type="password" name="user[password]" id="pword" required>
</div>
<input type="submit" value="LogIn" class="btn light-blue lighten-3">
</form>